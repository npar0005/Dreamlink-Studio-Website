  <!-- Site JS -->
  <script src="js/nav.js"></script>

  <!-- Site CSS -->
  <link rel="stylesheet" href="css/main.css" />
  
  <!-- From associated file, import the head block for specific page scripts, styles etc.-->
  {{{_sections.head}}}
</head>
<body>
  <main class="header-nav">
    <!-- Start Navigation -->
    <header>
      <nav class="navbar navbar-expand-md navbar-dark fixed-top">
        <div class="container-fluid">
          <a class="navbar-brand" href="index.html">
            <img src="images/logo.png" id="logo" alt="Dreamlink Studio Logo" />
          </a>
          <button type="button" class="navbar-toggler outline-none" data-toggle="collapse" data-target="#navbarSupportedContent">
            <span class="text-white navbar-toggler-icon"></span>
          </button>

          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav ml-auto">
              <li class="nav-item active px-3">
                <a href="index.html" class="nav-link">Home</a>
              </li>
              <li class="nav-item px-3">
                <a href="about.html" class="nav-link">About Us</a>
              </li>
              <li class="nav-item px-3">
                <a href="products.html" class="nav-link">Products</a>
              </li>
              <li class="nav-item px-3">
                <a href="contact.html" class="nav-link">Contact</a>
              </li>
            </ul>
          </div>
        </div>
      </nav>
    </header>
    <!-- End Navigation -->
  </main>

  {{{ body }}}

  <footer class="footer">
    <ul class="nav-links">
      <li><a href="#">Home</a></li>
      <li><a href="#">About</a></li>
      <li><a href="#">Products</a></li>
      <li><a href="#">Contact</a></li>
    </ul>
    <p>
      <i class="fas fa-moon mr-1"></i> &copy; 2020 Dreamlink Studio</p>
    </p>
  </footer>
  
</body>
</html>



